				<Methods>
					<Method>
						<Name>displayOption</Name>
						<Source><![CDATA[
        public void displayOption(Common _record, FormRowDisplayOption _options)
        {
            SalesTable table;
            table = _record;

            // Set control to apply design
            _options.affectedElementsByControl(Grid_SalesId.id());
            _options.affectedElementsByControl(Grid_SalesStatus.id());

            // Customize Grid for conditions
            if(table.SalesStatus == SalesStatus::Backorder)
            {
                _options.backColor(WinAPI::RGB2int(255,165,0));
            }
            else if(table.SalesStatus == SalesStatus::Invoiced)
            {
                _options.backColor(WinAPI::RGB2int(255,255,0));
                _options.fontItalic(true);
                _options.textColor(WinAPI::RGB2int(255,0,127 ));
                _options.fontStrikethrough(true);
            }
            else
            {
                _options.backColor(WinAPI::RGB2int(128,0,128));
                _options.fontUnderline(true);
            }

            super(_record, _options);
        }

]]></Source>
					</Method>
